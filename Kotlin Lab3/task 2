fun main() {
    println("Введите три числа:")
    val a = readLine()!!.toInt()
    val b = readLine()!!.toInt()
    val c = readLine()!!.toInt()
    
    if (a == b || b == c || a == c) {
        println("Ошибка")
    } else {
        val middle = when {
            (a > b && a < c) || (a < b && a > c) -> a
            (b > a && b < c) || (b < a && b > c) -> b
            else -> c
        }
        println("Среднее число: $middle")
    }
}
